table LINEITEM
{
"L_ORDERKEY" integer;
"L_PARTKEY" integer;
"L_SUPPKEY" integer;
"L_LINENUMBER" integer;
"L_QUANTITY" numeric (15, 2);
"L_EXTENDEDPRICE" numeric (15, 2);
"L_DISCOUNT" numeric (15, 2);
"L_TAX" numeric (15, 2);
"L_RETURNFLAG" char (1);
"L_LINESTATUS" char (1);
"L_SHIPDATE" date;
"L_COMMITDATE" date;
"L_RECEIPTDATE" date;
"L_SHIPINSTRUCT" char (25);
"L_SHIPMODE" char (10);
"L_COMMENT" varchar (44);
}
DATEFORM YYYY/MM/DD HH:MI:SS:SSSSSS
DATA_NLS_USE=US7ASCII
